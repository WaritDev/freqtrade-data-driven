
{
    "$schema": "https://schema.freqtrade.io/schema.json",
    "max_open_trades": 5,
    "stake_currency": "USDT",
    "stake_amount": "unlimited",
    "tradable_balance_ratio": 0.99,
    "fiat_display_currency": "USD",
    "dry_run": true,
    "dry_run_wallet": 1000,
    "cancel_open_orders_on_exit": false,
    "trading_mode": "futures",
    "margin_mode": "isolated",
    "unfilledtimeout": {
        "entry": 10,
        "exit": 10,
        "exit_timeout_count": 0,
        "unit": "minutes"
    },
    "entry_pricing": {
        "price_side": "same",
        "use_order_book": true,
        "order_book_top": 1,
        "price_last_balance": 0.0,
        "check_depth_of_market": {
            "enabled": false,
            "bids_to_ask_delta": 1
        }
    },
    "exit_pricing":{
        "price_side": "same",
        "use_order_book": true,
        "order_book_top": 1
    },
    "exchange": {
        "name": "binance",
        "key": "$BNB_API_KEY",
        "secret": "$BNB_API_SECRET",
        "ccxt_config": {},
        "ccxt_async_config": {},
        "pair_whitelist": [
            "ETH/USDT:USDT",
            "XRP/USDT:USDT",
            "BTC/USDT:USDT",
            "SOL/USDT:USDT",
            "SUI/USDT:USDT",
            "LTC/USDT:USDT",
            "ADA/USDT:USDT",
            "DOGE/USDT:USDT",
            "UNI/USDT:USDT",
            "LINK/USDT:USDT",
            "XLM/USDT:USDT",
            "AAVE/USDT:USDT",
            "BNB/USDT:USDT",
            "TRX/USDT:USDT",
            "IP/USDT:USDT"
        ],
        "pair_blacklist": [
        ]
    },
    "pairlists": [
    {"method": "StaticPairList"}
    ],
    "telegram": {
        "enabled": true,
        "token": "$TELEGRAM_TOKEN",
        "chat_id": "$TELEGRAM_CHAT"
    },
    "api_server": {
        "enabled": true,
        "listen_ip_address": "0.0.0.0",
        "listen_port": 8080,
        "verbosity": "error",
        "enable_openapi": false,
        "jwt_secret_key": "$API_JWT_SECRET",
        "ws_token": "$API_WS_TOKEN",
        "CORS_origins": [],
        "username": "$API_USER",
        "password": "$API_PASS"
    },
    "bot_name": "freqtrade",
    "initial_state": "running",
    "force_entry_enable": false,
    "internals": {
        "process_throttle_secs": 5
    }
}